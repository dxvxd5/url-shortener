{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-with-type-watch": "concurrently \"vite\" \"npm run type-generate-dev:watch\"",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-generate": "graphql-codegen",
    "type-generate-dev": "graphql-codegen --config codegen.dev.ts",
    "type-generate-dev:watch": "graphql-codegen --config codegen.dev.ts --watch",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@eslint/js": "^9.25.1",
    "@parcel/watcher": "^2.5.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "graphql": "^16.11.0",
    "jsdom": "^26.1.0",
    "prettier": "3.4.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.1.2"
  }
}
